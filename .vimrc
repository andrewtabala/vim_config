set autoindent
syntax on
set history=50
set ruler

set incsearch
set hlsearch

colorscheme badwolf

set tabstop=4
set softtabstop=4

set ai
set cin

set ignorecase

set number
set showcmd
set cursorline
set wildmenu
set lazyredraw
set showmatch
set foldenable
set foldlevelstart=10
set foldnestmax=10

